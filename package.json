{
    "name": "react-tutorial",
    "version": "0.0.1",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open --port 8080 --host localhost --history-api-fallback",
        "webpack": "webpack",
        "test": "jest"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/test/test-setup.js",
        "roots": [
            "./src"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "^[./a-zA-Z0-9$_-]+\\.(svg|png)": "<rootDir>/test/relativeImageStub.js"
        }
    },
    "repository": "git+https://github.com/seven-hills-technology/react-tutorial.git",
    "author": "Sean Price <seanprice@sevenhillstechnology.com>",
    "license": "MPL-2.0",
    "dependencies": {
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "react-redux": "^5.1.1",
        "redux": "^4.0.1",
        "redux-devtools-extension": "^2.13.5",
        "redux-form": "^7.4.2"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.15",
        "@types/jest": "^23.3.9",
        "@types/react-dom": "^16.0.9",
        "@types/react-redux": "^6.0.9",
        "@types/redux": "^3.6.0",
        "@types/redux-form": "^7.4.12",
        "@types/redux-thunk": "^2.1.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.7.0",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.4",
        "ts-loader": "^5.3.0",
        "typescript": "^3.1.6",
        "webpack": "^4.26.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    }
}
